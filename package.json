{
  "name": "gympass-backend",
  "version": "1.0.0",
  "main": "server.ts",
  "license": "MIT",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "prod": "node build/server.js",
    "build": "tsup src --out-dir build",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest run --dir src/use-cases",
    "pre:e2e": "run-s npm link ./prisma/vitest-environment-prisma && npm link vitest-environment-prisma",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:watch": "vitest watch --dir src/http",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/node": "18.14.6",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "@vitest/coverage-c8": "0.29.2",
    "@vitest/ui": "0.29.2",
    "eslint": "8.35.0",
    "eslint-plugin-import-helpers": "1.3.1",
    "npm-run-all": "4.1.5",
    "pino-pretty": "9.4.0",
    "prisma": "4.11.0",
    "supertest": "6.3.3",
    "tsup": "6.6.3",
    "tsx": "3.12.3",
    "typescript": "4.9.5",
    "vite-tsconfig-paths": "4.0.5",
    "vitest": "0.29.2"
  },
  "dependencies": {
    "@fastify/cookie": "8.3.0",
    "@fastify/jwt": "6.7.1",
    "@prisma/client": "4.11.0",
    "@types/supertest": "2.0.12",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.7",
    "dotenv": "16.0.3",
    "fastify": "4.14.1",
    "vitest-environment-prisma": "0.3.0",
    "zod": "3.21.4"
  }
}
